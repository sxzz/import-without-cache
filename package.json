{
  "name": "import-without-cache",
  "version": "0.1.1",
  "packageManager": "pnpm@10.24.0",
  "description": "Import ES modules without cache.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/import-without-cache#readme",
  "bugs": {
    "url": "https://github.com/sxzz/import-without-cache/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/import-without-cache.git"
  },
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "node --test tests/index.test.js",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.4.0",
    "@sxzz/prettier-config": "^2.2.6",
    "@types/node": "^24.10.1",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "prettier": "^3.7.3",
    "tsdown": "^0.17.0-beta.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "prettier": "@sxzz/prettier-config"
}
